    <formulas>
        <name>Is_QUEJA_Despatchos</name>
        <dataType>Boolean</dataType>
        <expression>(
ISPICKVAL({!$Record.Type},&apos;COMPAÑÍA&apos;) &amp;&amp; (ISPICKVAL({!$Record.ALK_Clase__c},&apos;ACCIDENTE&apos;) )
|| (ISPICKVAL({!$Record.Type},&apos;ENTREGA&apos;)&amp;&amp; (ISPICKVAL({!$Record.ALK_Clase__c},&apos;PRODUCTO TROCADO&apos;)|| ISPICKVAL({!$Record.ALK_Clase__c},&apos;FALSA ENTREGA&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;MERCANCÍA NO HAY&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;TRANSPORTADOR NO ALCANZA A TERMINAR RUTA&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;NO SALIÓ A RUTA&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;ENTREGA INCOMPLETA&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCUMPLIMIENTO&apos;))) 

|| (ISPICKVAL({!$Record.Type},&apos;PRODUCTOS INTANGIBLES&apos;)&amp;&amp; ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES CON LA INSTALACIÓN PROPIO&apos;))

|| (ISPICKVAL({!$Record.Type},&apos;PROCESO PAGO&apos;)&amp;&amp; ISPICKVAL({!$Record.ALK_Clase__c},&apos;TRANSACCIONES FRAUDULENTAS&apos;))
|| (ISPICKVAL({!$Record.Type},&apos;ENTREGA&apos;) &amp;&amp; (ISPICKVAL({!$Record.ALK_Clase__c},&apos;DOBLE ENTREGA&apos;) ))
|| (ISPICKVAL({!$Record.Type},&apos;RECOGE EN TIENDA&apos;)&amp;&amp; (ISPICKVAL({!$Record.ALK_Clase__c},&apos;NO LLEGO CORREO O MENSAJE DE CONFIRMACIÓN&apos;)))
|| (ISPICKVAL({!$Record.Type},&apos;TRANSPORTES&apos;)&amp;&amp; (ISPICKVAL({!$Record.ALK_Clase__c},&apos;ENTREGA INADECUADA TRANSPORTADOR&apos;)|| ISPICKVAL({!$Record.ALK_Clase__c},&apos;HURTO/DAÑO TRANSPORTADOR&apos;)|| ISPICKVAL({!$Record.ALK_Clase__c},&apos;VENTA PROHIBIDA TRANSPORTADOR&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;DESMONTE DE PUERTAS&apos;)))
)</expression>
    </formulas>
    <formulas>
        <name>Is_Valid_QUEJA_Producto_1</name>
        <dataType>Boolean</dataType>
        <expression>(
(ISPICKVAL({!$Record.Type},&apos;COMPAÑÍA&apos;) &amp;&amp; (ISPICKVAL({!$Record.ALK_Clase__c},&apos;ASESORÍA Y ATENCIÓN AL CLIENTE&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;ACCIDENTE CON PRODUCTO&apos;)))
|| (ISPICKVAL({!$Record.Type},&apos;SERVICIO&apos;)&amp;&amp; (ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES CON LA INSTALACIÓN MARCA&apos;)
                                                                                                                                        || ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES SEGURO SOAT&apos;)|| ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES SEGURO PERSONAL&apos;) 
                                                                                                                                        || ISPICKVAL({!$Record.ALK_Clase__c},&apos;CANCELACIÓN DEL SERVICIO&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES SEGURO CELULAR&apos;) 
                                                                                                                                        || ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES SEGURO GARANTÍA EXTENDIDA&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES ENTREGAS SIM CARD&apos;)))
|| (ISPICKVAL({!$Record.Type},&apos;COMPRA&apos;)&amp;&amp; (ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES EN EL PROCESO DE COMPRA INTERNET&apos;)|| ISPICKVAL({!$Record.ALK_Clase__c},&apos;FACTURA&apos;) || 
ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES EN EL PEDIDO INTERNET&apos;)|| 
ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONSISTENCIA EN PRECIOS&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;FACTURA DE COMPRA NO REALIZADA&apos;)|| ISPICKVAL({!$Record.ALK_Clase__c},&apos;FACTURA ELECTRONICA&apos;) ))
)</expression>
    </formulas>
    <formulas>
        <name>Is_Valid_QUEJA_Producto_2</name>
        <dataType>Boolean</dataType>
        <expression>((ISPICKVAL({!$Record.Type},&apos;PROCESO PAGO&apos;)&amp;&amp; (ISPICKVAL({!$Record.ALK_Clase__c},&apos;ERROR EN VUELTAS&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;PROMOCIÓN NO APLICADA&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;COBRO DE INTERESES TARJETA ALKOSTO&apos;)  
                                                             || ISPICKVAL({!$Record.ALK_Clase__c},&apos;COBRO DE INTERESES CRÉDITO 20 MINUTOS&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;DEMORAS EN EXTRACTOS&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;DEVOLUCIÓN DE CHEQUE&apos;)  
                                                             || ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONFORMIDAD PAGO ( FECHA-CUOTAS-MONTO-VALOR) TARJETA ALKOSTO&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONFORMIDAD PAGO ( FECHA-CUOTAS-MONTO-VALOR) CRÉDITO 20 MINUTOS&apos;) 
                                                             || ISPICKVAL({!$Record.ALK_Clase__c},&apos;EXENCIÓN IVA NO APLICADO&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;REPORTE EN CENTRALES DE RIESGO CRÉDITO 20 MINUTOS&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;REPORTE EN CENTRALES DE RIESGO TARJETA ALKOSTO&apos;) 
                                                             || ISPICKVAL({!$Record.ALK_Clase__c},&apos;DOBLE COBRO DE TRANSACCIÓN&apos;)))
|| (ISPICKVAL({!$Record.Type},&apos;SEGURIDAD&apos;)&amp;&amp; ISPICKVAL({!$Record.ALK_Clase__c},&apos;HURTO/DAÑO&apos;))
|| (ISPICKVAL({!$Record.Type},&apos;POSVENTA&apos;) ))</expression>
    </formulas>
    <formulas>
        <name>Is_Valid_QUEJA_Producto_3</name>
        <dataType>Boolean</dataType>
        <expression>(
(ISPICKVAL({!$Record.Type},&apos;PRODUCTOS INTANGIBLES&apos;) &amp;&amp; 
(ISPICKVAL({!$Record.ALK_Clase__c},&apos;CANCELACIÓN DEL SERVICIO&apos;)
|| ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES CON LA INSTALACIÓN PROPIO&apos;)
|| ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES CON LA INSTALACIÓN MARCA&apos;)
|| ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES SEGURO PERSONAL&apos;)
|| ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES SEGURO CELULAR&apos;)
|| ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES SEGURO GARANTÍA EXTENDIDA&apos;)
|| ISPICKVAL({!$Record.ALK_Clase__c},&apos;INCONVENIENTES ENTREGAS SIM CARD&apos;)))
)</expression>
    </formulas>
    <formulas>
        <name>Is_Valid_QUEJA_RMA</name>
        <dataType>Boolean</dataType>
        <expression>(ISPICKVAL({!$Record.Type},&apos;ENTREGA&apos;) &amp;&amp; (ISPICKVAL({!$Record.ALK_Clase__c},&apos;DEMORA EN LA RECOGIDA DEL PRODUCTO&apos;) ))</expression>
    </formulas>
    <formulas>
        <name>Is_Valid_SOLICITUD_DESPACHOS</name>
        <dataType>Boolean</dataType>
        <expression>(
ISPICKVAL({!$Record.Type},&apos;ENTREGA&apos;) &amp;&amp; (ISPICKVAL({!$Record.ALK_Clase__c},&apos;CAMBIO DE PRODUCTO NO ENTREGADO&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;CAMBIO FECHA DE DESPACHO&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;CAMBIO DE DIRECCIÓN DE DESPACHO&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;CANCELACIÓN DEL DESPACHO&apos;))
|| (ISPICKVAL({!$Record.Type},&apos;PRODUCTOS INTANGIBLES&apos;) &amp;&amp; ISPICKVAL({!$Record.ALK_Clase__c},&apos;INSTALACIÓN&apos;))
)</expression>
    </formulas>
    <formulas>
        <name>Is_Valid_SOLICITUD_Producto</name>
        <dataType>Boolean</dataType>
        <expression>(
ISPICKVAL({!$Record.Type},&apos;COMPAÑÍA&apos;) &amp;&amp; (ISPICKVAL({!$Record.ALK_Clase__c},&apos;DOCUMENTOS&apos;) || ISPICKVAL({!$Record.ALK_Clase__c},&apos;BENEFICIOS TRIBUTARIOS&apos;))

